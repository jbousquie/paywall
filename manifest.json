{
    "manifest_version": 3,
    "name": "Paywal Killer",
    "version": "1.0.0",
    "description": "Supprime le paywall sur les sites du groupe Midi Libre",
    "content_scripts": [
        {
          "js": ["content.js"],
          "matches": [ 
            "https://www.centrepresseaveyron.fr/*",
            "https://www.ladepeche.fr/*",
            "https://www.lindependant.fr/*",
            "https://www.midilibre.fr/*",
            "https://www.rugbyrama.fr/"
            ]
        }
    ],
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }